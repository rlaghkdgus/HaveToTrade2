  m_Script: {fileID: 11500000, guid: 64cd6ddb931a812489ac202d6a1f6fd2, type: 3}
  m_Name: Prologue
  m_EditorClassIdentifier: 
  Sheet1:
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uC791\uC740 \uD56D\uAD6C \uB9C8\uC744. \uC774\uB978 \uC544\uCE68, \uC548\uAC1C\uAC00
      \uC605\uAC8C \uB080 \uC2DC\uC7A5\uC5D0 \uC0AC\uB78C\uB4E4\uC774 \uC6C0\uC9C1\uC778\uB2E4.
      \uC8FC\uC778\uACF5\uC740 \uB9C8\uC9C0\uB9C9 \uC9D0\uC744 \uCC59\uAE30\uACE0
      \uC788\uB2E4."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uBB3C\uAC74 \uD655\uC778\u2026 \uAC00\uACA9\uD45C \uB2E4 \uBD99\uC600\uACE0\u2026
      \uC9C0\uB3C4, \uB098\uCE68\uBC18, \uBB3C, \uC2DD\uB7C9\uB3C4 \uCC59\uACBC\uACE0\u2026
      \uC88B\uC544. \uC774 \uC815\uB3C4\uBA74 \uCDA9\uBD84\uD574.\""
  - Type: Text
    SpeakerIndex: 2
    Name: "\uB9C8\uC744 \uC0C1\uC778"
    Dialog: "\"\uB9AC\uC5D0\uB098, \uC815\uB9D0 \uD63C\uC790 \uAC00\uACA0\uB2E4\uB294
      \uAC70\uB2C8? \uC544\uC9C1 \uC5B4\uB9AC\uC796\uC544. \uC544\uCE68 \uC548\uAC1C\uAC00
      \uAC77\uD788\uAE30 \uC804\uC5D0 \uCD9C\uBC1C\uD558\uBA74 \uC704\uD5D8\uD55C\uB370\u2026\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uAC71\uC815 \uB9C8\uC138\uC694! \uC624\uB298\uBD80\uD130\uB294 \uC9C4\uC9DC
      \uC7A5\uC0AC\uAFBC\uC774 \uB418\uAE30\uB85C \uD588\uAC70\uB4E0\uC694. \uB9C8\uC744\uC5D0\uC11C\uB9CC
      \uB3CC\uC544\uB2E4\uB2C8\uB294 \uAC78\uB85C\uB294 \uB354 \uC774\uC0C1 \uBC1C\uC804\uC774
      \uC5C6\uC5B4\uC694.\""
  - Type: Text
    SpeakerIndex: 2
    Name: "\uB9C8\uC744 \uC0C1\uC778"
    Dialog: "\"\uADF8\uB798\uB3C4 \uC232\uAE38\uC774 \uC694\uC998 \uC880 \uC774\uC0C1\uD558\uB2E8
      \uC18C\uBB38\uB3C4 \uC788\uACE0\u2026 \uBB34\uC5ED \uD5C8\uAC00\uC99D\uB3C4
      \uC774\uC81C \uB9C9 \uBC1B\uC740 \uAC70\uC796\uC544. \uAC70\uB798\uB3C4 \uCC98\uC74C\uC774\uC796\uB2C8?\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uADF8\uB7EC\uB2C8\uAE4C \uB354 \uB2A6\uAE30 \uC804\uC5D0 \uD574\uBD10\uC57C\uC8E0!
      \uC2DC\uC7A5\uC5D0\uC11C \uB0A8\uB294 \uBB3C\uAC74\uB4E4\uC744 \uC800\uCABD
      \uB9C8\uC744\uC5D0 \uC2F8\uAC8C \uD480\uBA74 \uBD84\uBA85 \uBA39\uD790 \uAC70\uC608\uC694.
      \uC800, \uC5B4\uC81C \uBC24\uC0C8 \uACC4\uC0B0\uB3C4 \uD574\uBD24\uC5B4\uC694!\""
  - Type: Text
    SpeakerIndex: 2
    Name: "\uB9C8\uC744 \uC0C1\uC778"
    Dialog: "\"\uADF8\uB7F0\uB370 \uB108 \uC9C0\uB09C\uBC88\uC5D4 \u2018\uAE38\uB208
      \uC790\uC2E0 \uC788\uB2E4\u2019\uACE0 \uD588\uB2E4\uAC00, \uC0BC\uB9BC \uACBD\uACC4
      \uB118\uC5B4\uAC14\uB358 \uAC70 \uAE30\uC5B5 \uC548 \uB098?\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uC544\uD558\uD558\u2026 \uC774\uBC88\uC5D4 \uC644\uC804 \uC900\uBE44\uD588\uC5B4\uC694!
      \uC9C0\uB3C4\uAC00 \uB450 \uC7A5\uC774\uB098 \uC788\uACE0, \uBC29\uD5A5\uD45C\uC2DC\uB3C4
      \uC9C1\uC811 \uB9CC\uB4E4\uC5B4\uB1A8\uC5B4\uC694!\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "(\uB098\uB294\u2026 \uC774\uC81C \uC9C4\uC9DC \uB0B4 \uD798\uC73C\uB85C
      \uBB54\uAC00\uB97C \uC774\uB8E8\uACE0 \uC2F6\uC5B4. \uB354 \uC774\uC0C1 \uC5B4\uB978\uB4E4
      \uADF8\uB9BC\uC790 \uB4A4\uC5D0 \uC228\uC9C0 \uC54A\uACE0\u2026 \uB098\uB9CC\uC758
      \uAE38\uC744 \uB9CC\uB4E4 \uAC70\uC57C.)"
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uC0BC\uB9BC \uC9C0\uC5ED, \uC790\uAC08\uAE38\uACFC \uC595\uC740 \uC5B8\uB355\uC774
      \uC774\uC5B4\uC9D0. \uAE38\uC774 \uC810\uCC28 \uD5D8\uD574\uC9C0\uACE0 \uC774\uC815\uD45C\uAC00
      \uC0AC\uB77C\uC9C4\uB2E4."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uC5E5\u2026 \uC65C \uBD81\uCABD\uC774 \uC774\uCABD\uC774\uC9C0? \uBD84\uBA85\uD788
      \uC800 \uC5B8\uB355\uC744 \uB118\uC73C\uBA74 \uB9C8\uC744\uC774 \uC788\uC5B4\uC57C
      \uD558\uB294\uB370\u2026\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\u2026\uB108\uBB34 \uC870\uC6A9\uD55C\uB370\u2026 \uD639\uC2DC \uC9C4\uC9DC
      \uAE38 \uC798\uBABB \uB4E0 \uAC74 \uC544\uB2C8\uACA0\uC9C0?\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uB364\uBD88 \uB4A4 \uAC70\uB300\uD55C \uBC31\uD638 \uC11D\uC0C1 \uBC1C\uACAC.
      \uC774\uB07C \uB080 \uB3CC\uC5D0 \uACE0\uB300 \uBB38\uC591\uC774 \uC0C8\uACA8\uC838
      \uC788\uC74C."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uC6B0\uC640\u2026 \uC774\uAC74\u2026 \uC9C4\uC9DC \uB300\uB2E8\uD558\uB2E4\u2026!
      \uD639\uC2DC, \uBCF4\uBB3C\uC778 \uAC74 \uC544\uB2C8\uACA0\uC9C0?\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uBB50\uC57C?! \uC9C4\uC9DC \uB3D9\uBB3C\uC774\uC57C?! \uC5B4\uB514\uC11C
      \uB098\uC628 \uAC70\uC57C \uB10C\u2026\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uBC31\uD638\uAC00 \uC870\uC6A9\uD788 \uB2E4\uAC00\uC640 \uB9AC\uC5D0\uB098
      \uACC1\uC5D0 \uC549\uB294\uB2E4."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\u2026\uACF5\uACA9\uD560 \uAC74 \uC544\uB2C8\uC9C0? \uADF8\uB0E5\u2026
      \uB0A0 \uBCF4\uB294 \uAC70\uC57C?\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uBC31\uD638\uAC00 \uAF2C\uB9AC\uB97C \uC0B4\uB791\uC774\uBA70 \uB9AC\uC5D0\uB098\uB97C
      \uB530\uB974\uAE30 \uC2DC\uC791\uD55C\uB2E4."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uADF8\uB798\u2026 \uBB50 \uC5B4\uB54C. \uD63C\uC790\uBCF4\uB2E8 \uB098\uC73C\uB2C8\uAE4C.
      \uB108\uB3C4\u2026 \uC678\uB85C\uC6E0\uB358 \uAC70\uC9C0?\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uC791\uC740 \uB0B4\uB959 \uB9C8\uC744 \uC2DC\uC7A5. \uD574\uAC00 \uC9C0\uAE30
      \uC804 \uB9C8\uC9C0\uB9C9 \uAC70\uB798\uAC00 \uC9C4\uD589\uB418\uACE0 \uC788\uB2E4."
  - Type: Text
    SpeakerIndex: 3
    Name: "\uC2DC\uC7A5 \uC0C1\uC778"
    Dialog: "\"\uCC98\uC74C \uBCF4\uB294 \uC0C1\uC778\uC774\uB124. \uC774 \uB3D9\uBB3C\uC740\u2026
      \uC560\uC644\uB3D9\uBB3C\uC774\uC57C?\""
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uC5B4\uCA4C\uB2E4 \uBCF4\uB2C8\u2026 \uC800\uB97C \uB530\uB77C\uC624\uAC8C
      \uB410\uC5B4\uC694. \uC774\uB984\uC740 \uC544\uC9C1 \uC5C6\uC9C0\uB9CC\uC694.
      \uC804 \uADF8\uB0E5\u2026 \uC624\uB298 \uC7A5\uC0AC\uBD80\uD130 \uD574\uBCF4\uB824\uACE0\uC694!\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uB9AC\uC5D0\uB098\uC758 \uCCAB \uAC70\uB798 \uC131\uC0AC. \uC2DC\uC7A5\uC744
      \uB458\uB7EC\uBCF4\uBA70 \uB9C8\uBB34\uB9AC."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uC624\uB298 \uD558\uB8E8\u2026 \uC9C4\uC9DC \uAE38\uC5C8\uC5B4. \uADF8\uB798\uB3C4
      \uB098, \uC870\uAE08\uC740 \uD574\uB0C8\uC9C0?\""
  - Type: Ex
    SpeakerIndex: 0
    Name: 
    Dialog: "\uC5B8\uB355 \uC704, \uB9AC\uC5D0\uB098\uAC00 \uBC31\uD638\uC640 \uD568\uAED8
      \uBC24\uD558\uB298\uC744 \uBC14\uB77C\uBD04."
  - Type: Text
    SpeakerIndex: 0
    Name: "\uC8FC\uC778\uACF5"
    Dialog: "\"\uD63C\uC790\uB294 \uBB34\uC12D\uC9C0 \uC54A\uB2E4\uACE0 \uC0DD\uAC01\uD588\uB294\uB370\u2026
      \uB124\uAC00 \uC788\uC5B4\uC11C, \uC9C4\uC9DC \uB2E4\uD589\uC774\uC57C.\""
